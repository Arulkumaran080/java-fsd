package method;

public class MethodOverloading {

	public int calculate(int a,int b)
	{
		return a+b;
	}
	public float calculate(int a)
	{
		int c = (22/7)*(a*a);
		return c;
	}
	public float calculate(float a,int b)
	{
		float c = a*b;
		return c;
	}
	public long calculate(long a)
	{
		return a*a;
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		MethodOverloading ab=new MethodOverloading();
		int a=ab.calculate(5,5);
		float b=ab.calculate(5);
		float c=ab.calculate(5f,7);
		long d=ab.calculate(8L);
		
		System.out.println(a);
		System.out.println(b);
		System.out.println(c);
		System.out.println(d);
	}

}
