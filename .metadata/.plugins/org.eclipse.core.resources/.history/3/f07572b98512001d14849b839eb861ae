package Thread;

class Threads extends Thread implements Runnable {
	
	public void run()
	{
		System.out.println("Thread demo ");
	}
}

public class threads {

	public static void main(String[] args) {
		
		//Thread class
		Threads mt=new Threads();
		mt.start();
		
		//Runnable Interface
		Threads mt1=new Threads();
		Thread t=new Thread(mt1);
		t.start();

	}

}