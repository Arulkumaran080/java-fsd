package subsequence;

public class Longsubsequence {

	public static void main(String[] args) {
//		int arr[]={5,8,3,15,23,13,52,65,64,52,80};
		int arr[] = {3, 10, 2, 1, 20};
		int n=arr.length;
		int max=0,i,count=0;
		for(i=0;i<n;i++){
			if(arr[i]>max){
				count++;
				max=arr[i];
			}
		}
		System.out.println(count);
	}

}
